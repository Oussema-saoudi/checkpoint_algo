ALGORITHM linera data structure
VAR
    distSum: INTEGER:=0;
    arr: ARRAY_OF INTEGER[100];
    set1,set2: arr;
BEGIN

Read(set1);
Read(set2);
FOR i FROM 0 TO set1.length STEP 1  DO
    FOR j FROM 0 TO set2.length STEP 1  DO
        IF (set1[i]<>set2[j]) THEN
            distSum:=distSum+set1[i];
        END_IF
    END_FOR
END_FOR

END